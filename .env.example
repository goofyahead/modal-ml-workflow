# Copy this file to .env and fill in your actual values

# Database connection (PostgreSQL with pgvector)
DATABASE_URL=postgresql://user:password@host/database?sslmode=require

# API Keys for AI services
GEMINI_API_KEY=your-gemini-api-key-here
ASSEMBLYAI_API_KEY=your-assemblyai-api-key-here
OPENAI_API_KEY=your-openai-api-key-here

# Authentication for endpoints
API_AUTH_KEY=your-secure-api-auth-key-here

# Modal workspace (for deployment)
MODAL_WORKSPACE=your-modal-workspace

# Optional: Hugging Face token (not needed with AssemblyAI)
# HUGGINGFACE_TOKEN=your-huggingface-token